### get access token
POST {{base_url_keycloak}}/realms/{{realm}}/protocol/openid-connect/token
Content-Type: application/x-www-form-urlencoded

grant_type=password&client_id=qunai-medical&client_secret={{client_secret}}&username=and2long&password=000000

> {%
client.global.set("access_token", response.body.access_token);
client.global.set("refresh_token", response.body.refresh_token);
%}

### refresh token
POST {{base_url_keycloak}}/realms/{{realm}}/protocol/openid-connect/token
Content-Type: application/x-www-form-urlencoded

grant_type=refresh_token&refresh_token={{refresh_token}}&client_id={{client_id}}&client_secret={{client_secret}}

> {%
client.global.set("access_token", response.body.access_token);
client.global.set("refresh_token", response.body.refresh_token);
%}

### create keycloak user
POST {{base_url_keycloak}}/admin/realms/{{realm}}/users
Content-Type: application/json
Authorization: Bearer {{access_token}}

{
  "enabled": true,
  "username": "username",
  "credentials": [
    {
      "type": "password",
      "value": "password",
      "temporary": false
    }
  ]
}
